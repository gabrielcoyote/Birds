<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Birds" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/Primary_dark</item>
        <item name="colorPrimaryVariant">@color/segundaria_dark</item>
        <item name="colorOnPrimary">@color/Black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/segundaria</item>
        <item name="colorSecondaryVariant">@color/segundaria_dark</item>
        <item name="colorOnSecondary">@color/Black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="android:navigationBarColor" >?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>